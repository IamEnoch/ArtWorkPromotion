<MudCard>
    <MudCardMedia Image="images/test.jpg" Height="200" />
    <MudCardContent>
        <MudText Typo="Typo.h5">@Name</MudText>
        <MudText Typo="Typo.h6">@ArtistName</MudText>
    </MudCardContent>
</MudCard>

@code{
    [Parameter]
    public string Name { get; set; }
     [Parameter]
    public string ArtistName { get; set; }
}